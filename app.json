{
  "expo": {
    "name": "meu-saldus",
    "slug": "meu-saldus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "meu-saldus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#212529",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#212529"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#212529"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-transparent.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
